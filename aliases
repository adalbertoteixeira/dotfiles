# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"
alias dockly="docker run --rm -it -v /var/run/docker.sock:/var/run/docker.sock lirantal/dockly"
alias webhint="docker run -it -v ~/.config/webhint/.hintrc:/app/.hintrc webhint"
alias go-dev="docker run -i  --rm --name go-dev -v ~/dev/go:/go go-dev"
alias tmux="TERM=screen-256color tmux"
alias="$HOME/dev/docker/ranger/ranger.py"
# dev cloud
alias docker-r="ssh -t $REMOTE_DOCKER 'docker run --rm -it -v /mnt/r:/src -e R_LIBS_USER=/src/lib  rocker/r-base'"
alias rtichoke="ssh -t $REMOTE_DOCKER 'docker run --rm -it -v /mnt/rtichoke:/src -e R_LIBS_USER=/src/lib rtichoke'"
alias ddgr="ssh -t $REMOTE_DOCKER 'docker run --rm -it ddgr'"
alias docker-rstudio="ssh -t $REMOTE_DOCKER 'docker run --rm -d -p 8787:8787 -v ~/r-studio:/src rocker/rstudio'"
alias docker-jq="ssh -t $REMOTE_DOCKER 'docker run --rm -i jq'"
